<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.testyourmath">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/launcher_icon"
        android:label="Test Your Math"
        android:largeHeap="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Animation_activity">

        </activity>

        <activity android:name=".Hard_classic" />
        <activity android:name=".medium_classic" />
        <activity android:name=".hard_timer" />
        <activity android:name=".medium_timer" />
        <activity android:name=".levels_of_game" />
        <activity android:name=".ClassicGame" />
        <activity android:name=".TypesOfGame" />
        <activity android:name=".Easy" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>